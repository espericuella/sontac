{
  "name": "sontac",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --port=4200 --host=0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "lint": "eslint --fix **/**.{ts,html}",
    "format": "prettier --write **/**.{ts,html}",
    "fix": "npm run format && npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-cli-ghpages": "^1.0.3",
    "autoprefixer": "^10.4.8",
    "postcss": "^8.4.14",
    "rxjs": "~7.5.0",
    "sass": "^1.54.3",
    "scss": "^0.2.4",
    "swiper": "^8.3.2",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular-eslint/builder": "17.1.0",
